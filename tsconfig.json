{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@composables/*": ["src/composables/*"],
      "@constants/*": ["src/constants/*"],
      "@utils/*": ["src/utils/*"],
      "@layout/*": ["src/layout/*"],
      "@assets/*": ["src/assets/*"],
      "@router/*": ["src/router/*"],
      "@data/*": ["src/data/*"],
      "@services/*": ["services/*"],
      "@type/*": ["types/*"],
      "@stores/*": ["src/stores/*"]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "include": ["src", "vite-env.d.ts"]
  }
}
